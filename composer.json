{
    "name": "uccello/uccello-api",
    "description": "A REST API for Uccello.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "laravel/sanctum": "^2.9",
        "uccello/uccello": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uccello\\Api\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\Api\\Providers\\AppServiceProvider",
                "Uccello\\Api\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
